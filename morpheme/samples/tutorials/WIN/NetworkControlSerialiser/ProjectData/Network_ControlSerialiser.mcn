						<ScriptData type="string"><![CDATA[------------------------------------------------------------------------------------------------------------------------
-- include some useful utilities
-- RayCastToolPreview.lua is required for the default ray cast tool functionality
------------------------------------------------------------------------------------------------------------------------
require "previewScripts/DebugDraw.lua"
require "previewScripts/RayCastToolPreview.lua"
require "previewScripts/VectorMath.lua"
------------------------------------------------------------------------------------------------------------------------

runUpdateScript = false

SmoothSpeed = 0
SmoothBanking = 0
increment = 0.02


local keyDownHandler = function(key)
  if key == "space" then
    runUpdateScript = not runUpdateScript
    if(runUpdateScript) then
      print("Running update script")
    else
      print("Stopped update script")
    end
  end
end

local updateScript = function()

  if (runUpdateScript == true ) then 

    speed = -joy.y
    turn = joy.x

    --change the speed parameter
    setControlParam("Speed", (speed+1)/2)



    -- determine whether or no to do a quick turn
    if (turn > SmoothBanking) then	
    
      diff = turn - SmoothBanking
      if (diff > 0.5) then
        doFastTurn = true
      else
        doFastTurn = false
      end
      if(diff > increment) then
        SmoothBanking = SmoothBanking + increment
      end
    else
      diff = SmoothBanking - turn 
      if (diff > 0.5) then
        doFastTurn = true
      else
        doFastTurn = false
      end
      if(diff > increment) then
        SmoothBanking = SmoothBanking - increment
      end
    end
    
    --change the banking parameter
    setControlParam("Banking", (turn+1)/2)


    -- navigat the states
    currentState = getCurrentState()

    if(currentState == "Idle") then 
      if(speed>0.15) then
        broadcastRequest("ForwardsLeadLeft")    
      end
    end

    if(currentState == "Forwards") then 
      if(speed<0.15) then
        broadcastRequest("Idle")    
      end
    end

    if(currentState == "Forwards") then  
      if (speed>0) then
        if(doFastTurn) then
          if(turn<0) then
            broadcastRequest("TurnLeft")
          else
            broadcastRequest("TurnRight")
          end
        end
      elseif (speed==0) then
        broadcastRequest("Idle")
      end
    end

  end -- end check for running script
end

registerKeyDownEventHandler(keyDownHandler)
registerUpdateHandler(updateScript)
]]></ScriptData>
